{
    "hosting": {
        "public": "public",
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Access-Control-Allow-Origin",
                        "value": "https://your-frontend-domain.com"
                    },
                    {
                        "key": "Access-Control-Allow-Credentials",
                        "value": "true"
                    }
                ]
            }
        ]
    }
}